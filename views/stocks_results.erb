		<div class="photo">
			<img src="<%= @img_url %>" >
		</div>
		
		<div class="about">
			<div>
				<ul><h3>Here is the summary for <%= @stock_symbol %>:</h3>
					<li>Company: <%= @stock_company %> </li>
					<li>Today's High: <%= @stock_high %> </li>
					<li>Today's Low: <%= @stock_low %> </li>
					<li>Open: <%= @stock_open %> </li>
					<li>Volume: <%= @stock_volume %> </li>				
				</ul>
			</div>
			<form action="/stocks_results" method="post">
				<legend>Search for a stock by symbol</legend>
				<div class="input-append">
					<input type="text" name="symbol" value="<%= @symbol %>" class="span2" id="appendedInputButton">
					<button class="btn">Search</button>
				</div>
			</form>
		</div>